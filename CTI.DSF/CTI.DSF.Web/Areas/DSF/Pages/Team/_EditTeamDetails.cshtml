@using Models
@model TeamViewModel

<div class="row row-cols-1 row-cols-sm-1 row-cols-md-1 row-cols-lg-1 row-cols-xl-1 row-cols-xxl-1">
    <div class="col">
	<div class="mb-3">
		<label asp-for="@Model.SectionCode" class="form-label "><strong>@Localizer["Section Code"] :</strong></label>
			<input asp-for="ForeignKeySection" class="form-control"   readonly />
			<span asp-validation-for="@Model.SectionCode" class="text-danger"></span>
			<input type="hidden" asp-for="@Model.SectionCode"/>
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.TeamCode" class="form-label fieldrequired"><strong>@Localizer["Team Code"] :</strong></label>
			<input asp-for="@Model.TeamCode" class="form-control"  />
			<span asp-validation-for="@Model.TeamCode" class="text-danger"></span>
			
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.TeamName" class="form-label fieldrequired"><strong>@Localizer["Team Name"] :</strong></label>
			<input asp-for="@Model.TeamName" class="form-control"  />
			<span asp-validation-for="@Model.TeamName" class="text-danger"></span>
			
	</div>
</div>

</div>
