@using Models
@model BatchViewModel

<div class="row row-cols-1 row-cols-sm-1 row-cols-md-1 row-cols-lg-1 row-cols-xl-1 row-cols-xxl-1">
    <div class="col">
	<div class="mb-3">
		<label asp-for="@Model.Date" class="form-label fieldrequired"><strong>@Localizer["Date"] :</strong></label>
			<input asp-for="@Model.Date" class="form-control"  />
			<span asp-validation-for="@Model.Date" class="text-danger"></span>
			
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.Batch" class="form-label fieldrequired"><strong>@Localizer["Batch"] :</strong></label>
			<input asp-for="@Model.Batch" class="form-control"  />
			<span asp-validation-for="@Model.Batch" class="text-danger"></span>
			
	</div>
</div>

</div>
