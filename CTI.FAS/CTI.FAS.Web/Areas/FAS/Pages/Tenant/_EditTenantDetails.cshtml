@using Models
@model TenantViewModel

<div class="row row-cols-1 row-cols-sm-1 row-cols-md-1 row-cols-lg-1 row-cols-xl-1 row-cols-xxl-1">
    <div class="col">
	<div class="mb-3">
		<label asp-for="@Model.ProjectId" class="form-label fieldrequired"><strong>@Localizer["Project"] :</strong></label>
			<input asp-for="ForeignKeyProject" class="form-control"   readonly />
			<span asp-validation-for="@Model.ProjectId" class="text-danger"></span>
			<input type="hidden" asp-for="@Model.ProjectId"/>
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.Name" class="form-label fieldrequired"><strong>@Localizer["Name"] :</strong></label>
			<input asp-for="@Model.Name" class="form-control" maxlength="255"  readonly/>
			<span asp-validation-for="@Model.Name" class="text-danger"></span>
			
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.Code" class="form-label fieldrequired"><strong>@Localizer["Code"] :</strong></label>
			<input asp-for="@Model.Code" class="form-control" maxlength="255"  readonly/>
			<span asp-validation-for="@Model.Code" class="text-danger"></span>
			
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.DateFrom" class="form-label fieldrequired"><strong>@Localizer["Date From"] :</strong></label>
			<input type="date" asp-for="@Model.DateFrom" class="form-control"  readonly/>
			<span asp-validation-for="@Model.DateFrom" class="text-danger"></span>
			
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.DateTo" class="form-label fieldrequired"><strong>@Localizer["Date To"] :</strong></label>
			<input type="date" asp-for="@Model.DateTo" class="form-control"  readonly/>
			<span asp-validation-for="@Model.DateTo" class="text-danger"></span>
			
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.IsDisabled" class="form-label "><strong>@Localizer["Disabled"] :</strong></label>
			<input asp-for="@Model.IsDisabled" class="custom-checkbox-label"/>
			<span asp-validation-for="@Model.IsDisabled" class="text-danger"></span>
			
	</div>
</div>

</div>
