@using Models
@model ProjectViewModel

<div class="row row-cols-1 row-cols-sm-1 row-cols-md-1 row-cols-lg-1 row-cols-xl-1 row-cols-xxl-1">
    <div class="col">
	<div class="mb-3">
		<label asp-for="@Model.CompanyId" class="form-label fieldrequired"><strong>@Localizer["Entity"] :</strong></label>
			<input asp-for="ForeignKeyCompany" class="form-control"   readonly />
			<span asp-validation-for="@Model.CompanyId" class="text-danger"></span>
			<input type="hidden" asp-for="@Model.CompanyId"/>
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.Name" class="form-label fieldrequired"><strong>@Localizer["Name"] :</strong></label>
			<input asp-for="@Model.Name" class="form-control" maxlength="255"  readonly/>
			<span asp-validation-for="@Model.Name" class="text-danger"></span>
			
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.Code" class="form-label fieldrequired"><strong>@Localizer["Code"] :</strong></label>
			<input asp-for="@Model.Code" class="form-control" maxlength="20"  readonly/>
			<span asp-validation-for="@Model.Code" class="text-danger"></span>
			
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.ProjectAddress" class="form-label "><strong>@Localizer["Address"] :</strong></label>
			<input asp-for="@Model.ProjectAddress" class="form-control" maxlength="255" />
			<span asp-validation-for="@Model.ProjectAddress" class="text-danger"></span>
			
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.Location" class="form-label "><strong>@Localizer["Location"] :</strong></label>
			<input asp-for="@Model.Location" class="form-control" maxlength="100" />
			<span asp-validation-for="@Model.Location" class="text-danger"></span>
			
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.LandArea" class="form-label fieldrequired"><strong>@Localizer["Land Area"] :</strong></label>
			<input asp-for="@Model.LandArea" class="form-control"  />
			<span asp-validation-for="@Model.LandArea" class="text-danger"></span>
			
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.GLA" class="form-label fieldrequired"><strong>@Localizer["GLA"] :</strong></label>
			<input asp-for="@Model.GLA" class="form-control"  />
			<span asp-validation-for="@Model.GLA" class="text-danger"></span>
			
	</div>
</div>
<div class="col">
	<div class="mb-3">
		<label asp-for="@Model.IsDisabled" class="form-label "><strong>@Localizer["Disabled"] :</strong></label>
			<input asp-for="@Model.IsDisabled" class="custom-checkbox-label"/>
			<span asp-validation-for="@Model.IsDisabled" class="text-danger"></span>
			
	</div>
</div>

</div>
