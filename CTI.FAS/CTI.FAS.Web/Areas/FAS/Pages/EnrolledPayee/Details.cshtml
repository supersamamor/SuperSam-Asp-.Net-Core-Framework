@page
@model DetailsModel
@{
    ViewData["Title"] = Localizer["View Payee Enrollment"];
    ViewData["ActiveMainPage"] = "EnrolledPayee";
    ViewData["Level1"] = "AP";
}
<div class="container-fluid" id="enrolledPayee-container">
    <input type="hidden" asp-for="@Model.RemoveSubDetailId" />
    <input type="hidden" asp-for="@Model.AsyncAction" />
    <div id="PartialLoaderContainer">
        <partial name="_InputFieldsPartial" model="Model.EnrolledPayee" />
    </div>
</div>
<div id="toolbar-container" class="btn-group" role="group" style="float:right;margin-top:10px;">
    <a asp-page="ActivePayee" class="btn btn-secondary" title="@Localizer["Close"]"><i class="fas fa-ban"></i></a>    
</div>
<a id="back-to-top" href="#" class="btn btn-primary back-to-top" role="button" aria-label="Scroll to top">
    <i class="fas fa-chevron-up"></i>
</a>
@section Scripts {
<partial name="_DisableFieldsPartial" model="@("enrolledPayee-container")" />
}