@using Models
@model RevalidateViewModel

<div class="col-md-12">
	<label asp-for="@Model.SalesDate" class="fieldrequired"></label>
	<input type="date" asp-for="@Model.SalesDate" class="form-control"  />
	<span asp-validation-for="@Model.SalesDate" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.ProjectId" class="fieldrequired"></label>
	<select asp-for="@Model.ProjectId" class="form-control" asp-items="@(dropdownService.GetProjectList(Model.ProjectId))"><option value="">- Select Project -</option></select>
	<span asp-validation-for="@Model.ProjectId" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.TenantId" class=""></label>
	<select asp-for="@Model.TenantId" class="form-control" asp-items="@(dropdownService.GetTenantList(Model.TenantId))"><option value="">- Select Tenant -</option></select>
	<span asp-validation-for="@Model.TenantId" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.Status" class="fieldrequired"></label>
	<input asp-for="@Model.Status" class="form-control" maxlength="50"  readonly/>
	<span asp-validation-for="@Model.Status" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.ProcessingRemarks" class=""></label>
	<input asp-for="@Model.ProcessingRemarks" class="form-control"   readonly/>
	<span asp-validation-for="@Model.ProcessingRemarks" class="text-danger"></span>
	
</div>

