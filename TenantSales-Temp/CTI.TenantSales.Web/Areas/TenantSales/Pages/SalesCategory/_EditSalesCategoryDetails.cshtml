@using Models
@model SalesCategoryViewModel

<div class="col-md-12">
	<label asp-for="@Model.Code" class="fieldrequired"></label>
	<input asp-for="@Model.Code" class="form-control" maxlength="20"  readonly/>
	<span asp-validation-for="@Model.Code" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.Name" class="fieldrequired"></label>
	<input asp-for="@Model.Name" class="form-control" maxlength="100"  readonly/>
	<span asp-validation-for="@Model.Name" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.Rate" class="fieldrequired"></label>
	<input asp-for="@Model.Rate" class="form-control"   readonly/>
	<span asp-validation-for="@Model.Rate" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.TenantId" class="fieldrequired"></label>
	<input  asp-for="ForeignKeyTenant" class="form-control"   readonly />
	<span asp-validation-for="@Model.TenantId" class="text-danger"></span>
	<input type="hidden" asp-for="@Model.TenantId"/>
</div>
<div class="col-md-12">
	<label asp-for="@Model.IsDisabled" class=""></label>
	<input asp-for="@Model.IsDisabled" class="custom-checkbox-label"/>
	<span asp-validation-for="@Model.IsDisabled" class="text-danger"></span>
	
</div>

