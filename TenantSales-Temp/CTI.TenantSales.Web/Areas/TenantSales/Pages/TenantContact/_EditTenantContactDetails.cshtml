@using Models
@model TenantContactViewModel

<div class="col-md-12">
	<label asp-for="@Model.Group" class="fieldrequired"></label>
	<input asp-for="@Model.Group" class="form-control"  />
	<span asp-validation-for="@Model.Group" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.Type" class="fieldrequired"></label>
	<input asp-for="@Model.Type" class="form-control"  />
	<span asp-validation-for="@Model.Type" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.Detail" class=""></label>
	<input asp-for="@Model.Detail" class="form-control" maxlength="50" />
	<span asp-validation-for="@Model.Detail" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.TenantId" class="fieldrequired"></label>
	<select asp-for="@Model.TenantId" class="form-control" asp-items="@(dropdownService.GetTenantList(Model.TenantId))"><option value="">- Select Tenant -</option></select>
	<span asp-validation-for="@Model.TenantId" class="text-danger"></span>
	
</div>

