@using Models
@model CompanyViewModel

<div class="col-md-12">
	<label asp-for="@Model.Name" class="fieldrequired"></label>
	<input asp-for="@Model.Name" class="form-control" maxlength="255"  readonly/>
	<span asp-validation-for="@Model.Name" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.Code" class="fieldrequired"></label>
	<input asp-for="@Model.Code" class="form-control" maxlength="5"  readonly/>
	<span asp-validation-for="@Model.Code" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.EntityAddress" class=""></label>
	<input asp-for="@Model.EntityAddress" class="form-control" maxlength="255"  readonly/>
	<span asp-validation-for="@Model.EntityAddress" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.EntityAddressSecondLine" class=""></label>
	<input asp-for="@Model.EntityAddressSecondLine" class="form-control" maxlength="255"  readonly/>
	<span asp-validation-for="@Model.EntityAddressSecondLine" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.EntityDescription" class=""></label>
	<input asp-for="@Model.EntityDescription" class="form-control" maxlength="100" />
	<span asp-validation-for="@Model.EntityDescription" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.EntityShortName" class=""></label>
	<input asp-for="@Model.EntityShortName" class="form-control" maxlength="20" />
	<span asp-validation-for="@Model.EntityShortName" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.IsDisabled" class=""></label>
	<input asp-for="@Model.IsDisabled" class="custom-checkbox-label"/>
	<span asp-validation-for="@Model.IsDisabled" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.TINNo" class=""></label>
	<input asp-for="@Model.TINNo" class="form-control" maxlength="17"  readonly/>
	<span asp-validation-for="@Model.TINNo" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.DatabaseConnectionSetupId" class="fieldrequired"></label>
	<input  asp-for="ForeignKeyDatabaseConnectionSetup" class="form-control"   readonly />
	<span asp-validation-for="@Model.DatabaseConnectionSetupId" class="text-danger"></span>
	<input type="hidden" asp-for="@Model.DatabaseConnectionSetupId"/>
</div>

