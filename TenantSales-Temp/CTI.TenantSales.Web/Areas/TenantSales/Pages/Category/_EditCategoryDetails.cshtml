@using Models
@model CategoryViewModel

<div class="col-md-12">
	<label asp-for="@Model.Name" class="fieldrequired"></label>
	<input asp-for="@Model.Name" class="form-control" maxlength="80"  readonly/>
	<span asp-validation-for="@Model.Name" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.ClassificationId" class="fieldrequired"></label>
	<input  asp-for="ForeignKeyClassification" class="form-control"   readonly />
	<span asp-validation-for="@Model.ClassificationId" class="text-danger"></span>
	<input type="hidden" asp-for="@Model.ClassificationId"/>
</div>
<div class="col-md-12">
	<label asp-for="@Model.Code" class="fieldrequired"></label>
	<input asp-for="@Model.Code" class="form-control" maxlength="15"  readonly/>
	<span asp-validation-for="@Model.Code" class="text-danger"></span>
	
</div>

