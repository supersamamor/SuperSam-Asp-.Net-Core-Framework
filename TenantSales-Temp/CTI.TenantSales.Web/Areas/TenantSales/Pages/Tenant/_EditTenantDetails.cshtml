@using Models
@model TenantViewModel

<div class="row">
    <div class="col-7">
        <div class="row">
            <div class="col-7">
                <div class="card card-outline card-primary">
                    <div class="card-header d-flex">
                        <div class="card-title flex-grow-1">
                            <button class="btn btn-block text-left text-black p-0" type="button" data-card-widget="collapse">
                                <h5>@Localizer["Tenant Info"]</h5>
                            </button>
                        </div>
                        <div class="card-tools">
                            <button type="button" class="btn btn-tool" data-card-widget="collapse">
                                <i class="fas fa-minus"></i>
                            </button>
                        </div>
                    </div>
                    <div class="card-body">
                        <input type="hidden" asp-for="@Model.Id" name="Id" />
                        <div class="row">
                            <div class="col-md-6">
                                <label asp-for="@Model.Name" class="fieldrequired"></label>
                                <input asp-for="@Model.Name" class="form-control" maxlength="255" readonly />
                                <span asp-validation-for="@Model.Name" class="text-danger"></span>
                            </div>
                            <div class="col-md-6">
                                <label asp-for="@Model.Code" class="fieldrequired"></label>
                                <input asp-for="@Model.Code" class="form-control" maxlength="255" readonly />
                                <span asp-validation-for="@Model.Code" class="text-danger"></span>
                            </div>
                            <div class="col-md-6">
                                <label asp-for="@Model.RentalTypeId" class="fieldrequired"></label>
                                <input asp-for="ForeignKeyRentalType" class="form-control" readonly />
                                <span asp-validation-for="@Model.RentalTypeId" class="text-danger"></span>
                                <input type="hidden" asp-for="@Model.RentalTypeId" />
                            </div>
                            <div class="col-md-6">
                                <label asp-for="@Model.ProjectId" class="fieldrequired"></label>
                                <input asp-for="ForeignKeyProject" class="form-control" readonly />
                                <span asp-validation-for="@Model.ProjectId" class="text-danger"></span>
                                <input type="hidden" asp-for="@Model.ProjectId" />
                            </div>
                            <div class="col-md-6">
                                <label asp-for="@Model.LevelId" class=""></label>
                                <input asp-for="ForeignKeyLevel" class="form-control" readonly />
                                <span asp-validation-for="@Model.LevelId" class="text-danger"></span>
                                <input type="hidden" asp-for="@Model.LevelId" />
                            </div>
                            <div class="col-md-6">
                                <label asp-for="@Model.FileCode" class=""></label>
                                <input asp-for="@Model.FileCode" class="form-control" maxlength="20" />
                                <span asp-validation-for="@Model.FileCode" class="text-danger"></span>
                            </div>
                            <div class="col-md-6">
                                <label asp-for="@Model.Folder" class=""></label>
                                <input asp-for="@Model.Folder" class="form-control" maxlength="20" />
                                <span asp-validation-for="@Model.Folder" class="text-danger"></span>
                            </div>
                            <div class="col-md-6">
                                <label asp-for="@Model.IsDisabled" class=""></label>
                                <input asp-for="@Model.IsDisabled" class="custom-checkbox-label" />
                                <span asp-validation-for="@Model.IsDisabled" class="text-danger"></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-5">
                <div class="card card-outline card-primary">
                    <div class="card-header d-flex">
                        <div class="card-title flex-grow-1">
                            <button class="btn btn-block text-left text-black p-0" type="button" data-card-widget="collapse">
                                <h5>@Localizer["Commencement"]</h5>
                            </button>
                        </div>
                        <div class="card-tools">
                            <button type="button" class="btn btn-tool" data-card-widget="collapse">
                                <i class="fas fa-minus"></i>
                            </button>
                        </div>
                    </div>
                    <div class="card-body">
                        <input type="hidden" asp-for="@Model.Id" name="Id" />
                        <div class="row">
                            <div class="col-md-12">
                                <label asp-for="@Model.DateFrom" class="fieldrequired"></label>
                                <input type="date" asp-for="@Model.DateFrom" class="form-control" readonly />
                                <span asp-validation-for="@Model.DateFrom" class="text-danger"></span>
                            </div>
                            <div class="col-md-12">
                                <label asp-for="@Model.DateTo" class="fieldrequired"></label>
                                <input type="date" asp-for="@Model.DateTo" class="form-control" readonly />
                                <span asp-validation-for="@Model.DateTo" class="text-danger"></span>

                            </div>
                            <div class="col-md-12">
                                <label asp-for="@Model.Opening" class="fieldrequired"></label>
                                <input type="date" asp-for="@Model.Opening" class="form-control" readonly />
                                <span asp-validation-for="@Model.Opening" class="text-danger"></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-12">
                <partial name="_SalesCategoryList" model="Model" />
                <partial name="_TenantPOSList" model="Model" />
                <partial name="_TenantLotList" model="Model" />
            </div>
        </div>
    </div>
    <div class="col-5">
        <div class="card card-outline card-primary">
            <div class="card-header d-flex">
                <div class="card-title flex-grow-1">
                    <button class="btn btn-block text-left text-black p-0" type="button" data-card-widget="collapse">
                        <h5>@Localizer["Contact Information"]</h5>
                    </button>
                </div>
                <div class="card-tools">
                    <button type="button" class="btn btn-tool" data-card-widget="collapse">
                        <i class="fas fa-minus"></i>
                    </button>
                </div>
            </div>
            <div class="card-body">
                <input type="hidden" asp-for="@Model.Id" name="Id" />
                <div class="row">
                    <input type="hidden" id="TenantContactCount" value="@Model?.TenantContactList?.Count" />
                    <partial name="_TenantBranchContactList" model="Model" />
                    <partial name="_TenantHeadOfficeContactList" model="Model" />
                    <partial name="_TenantITSupportContactList" model="Model" />
                </div>
            </div>
        </div>
    </div>
</div>


