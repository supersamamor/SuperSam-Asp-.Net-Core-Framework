@using Models
@model DeliverableViewModel

<div class="col-md-12">
	<label asp-for="@Model.ProjectCategoryId" class="fieldrequired"></label>
	<select asp-for="@Model.ProjectCategoryId" class="form-control" asp-items="@(dropdownService.GetProjectCategoryList(Model.ProjectCategoryId))"><option value="">- Select Project Category -</option></select>
	<span asp-validation-for="@Model.ProjectCategoryId" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.Name" class="fieldrequired"></label>
	<input asp-for="@Model.Name" class="form-control" maxlength="255" />
	<span asp-validation-for="@Model.Name" class="text-danger"></span>
	
</div>

