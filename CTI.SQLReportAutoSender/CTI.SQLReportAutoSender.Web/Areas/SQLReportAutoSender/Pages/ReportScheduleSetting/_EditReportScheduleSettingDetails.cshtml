@using Models
@model ReportScheduleSettingViewModel

<div class="col-md-12">
	<label asp-for="@Model.ReportId" class="fieldrequired"></label>
	<select asp-for="@Model.ReportId" class="form-control" asp-items="@(dropdownService.GetReportList(Model.ReportId))"><option value="">- Select  -</option></select>
	<span asp-validation-for="@Model.ReportId" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.ScheduleFrequencyId" class="fieldrequired"></label>
	<select asp-for="@Model.ScheduleFrequencyId" class="form-control" asp-items="@(dropdownService.GetScheduleFrequencyList(Model.ScheduleFrequencyId))"><option value="">- Select  -</option></select>
	<span asp-validation-for="@Model.ScheduleFrequencyId" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.ScheduleParameterId" class="fieldrequired"></label>
	<select asp-for="@Model.ScheduleParameterId" class="form-control" asp-items="@(dropdownService.GetScheduleParameterList(Model.ScheduleParameterId))"><option value="">- Select  -</option></select>
	<span asp-validation-for="@Model.ScheduleParameterId" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.Value" class="fieldrequired"></label>
	<input asp-for="@Model.Value" class="form-control" maxlength="50" />
	<span asp-validation-for="@Model.Value" class="text-danger"></span>
	
</div>

