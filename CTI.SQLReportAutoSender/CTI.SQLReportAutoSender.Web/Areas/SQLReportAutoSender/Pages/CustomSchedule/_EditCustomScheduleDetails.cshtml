@using Models
@model CustomScheduleViewModel

<div class="col-md-12">
	<label asp-for="@Model.ReportId" class="fieldrequired"></label>
	<select asp-for="@Model.ReportId" class="form-control" asp-items="@(dropdownService.GetReportList(Model.ReportId))"><option value="">- Select  -</option></select>
	<span asp-validation-for="@Model.ReportId" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.SequenceNumber" class="fieldrequired"></label>
	<input asp-for="@Model.SequenceNumber" class="form-control"  />
	<span asp-validation-for="@Model.SequenceNumber" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.DateTimeSchedule" class="fieldrequired"></label>
	<input asp-for="@Model.DateTimeSchedule" class="form-control"  />
	<span asp-validation-for="@Model.DateTimeSchedule" class="text-danger"></span>
	
</div>

