@page
@model DeleteModel
@{
    ViewData["Title"] = Localizer["Delete Mail Setting"];
    ViewData["ActiveMainPage"] = "MailSetting";
    ViewData["Level1"] = "SQLReportAutoSender";
}
<div class=row>
    <div class="col-12">
        <div class="alert alert-danger" role="alert">
            @Localizer["Are you sure you want to delete this mailSetting?"]
        </div>
    </div>
</div>
<form id="mailSetting-form" method="post">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
	<input type="hidden" asp-for="@Model.RemoveSubDetailId" />
    <input type="hidden" asp-for="@Model.AsyncAction" />
	<div id="PartialLoaderContainer">
        <partial name="_InputFieldsPartial" model="Model.MailSetting" />
    </div> 
    <div class="row">
        <div class="col-12">
            <p>
                <a asp-page="Index" class="btn btn-secondary">@Localizer["Cancel"]</a>
                <button type="submit" class="btn btn-danger">@Localizer["Delete"]</button>
            </p>
        </div>
    </div>
</form>
<a id="back-to-top" href="#" class="btn btn-primary back-to-top" role="button" aria-label="Scroll to top">
    <i class="fas fa-chevron-up"></i>
</a>
@section Scripts {
<partial name="_DisableFieldsPartial" model="@("mailSetting-form")" />
}