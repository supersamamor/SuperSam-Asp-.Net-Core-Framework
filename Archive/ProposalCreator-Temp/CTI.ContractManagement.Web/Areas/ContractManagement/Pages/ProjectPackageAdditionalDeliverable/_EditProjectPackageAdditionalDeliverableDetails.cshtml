@using Models
@model ProjectPackageAdditionalDeliverableViewModel

<div class="col-md-12">
	<label asp-for="@Model.ProjectPackageId" class="fieldrequired"></label>
	<select asp-for="@Model.ProjectPackageId" class="form-control" asp-items="@(dropdownService.GetProjectPackageList(Model.ProjectPackageId))"><option value="">- Select Project Package -</option></select>
	<span asp-validation-for="@Model.ProjectPackageId" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.DeliverableId" class="fieldrequired"></label>
	<select asp-for="@Model.DeliverableId" class="form-control" asp-items="@(dropdownService.GetDeliverableList(Model.DeliverableId))"><option value="">- Select Deliverable -</option></select>
	<span asp-validation-for="@Model.DeliverableId" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.Sequence" class=""></label>
	<input asp-for="@Model.Sequence" class="form-control"  />
	<span asp-validation-for="@Model.Sequence" class="text-danger"></span>
	
</div>

