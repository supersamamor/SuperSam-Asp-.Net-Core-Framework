@using Models
@model MainModuleViewModel

<div class="col-md-12">
    <label asp-for="@Model.Code" class="fieldrequired"></label>
    <input asp-for="@Model.Code" class="form-control" maxlength="255" />
    <span asp-validation-for="@Model.Code" class="text-danger"></span>
</div>
<div class="col-md-12">
    <label asp-for="@Model.ParentModuleId" class="fieldrequired"></label>
    <select asp-for="@Model.ParentModuleId" class="form-control" asp-items="@(await dropdownService.GetParentModuleList())"><option value="">- Select ParentModule -</option></select>
    <span asp-validation-for="@Model.ParentModuleId" class="text-danger"></span>
</div>

<input asp-for="@Model.FileUpload" class="DocumentFile" />