@using Models
@model SubDetailItemPlaceHolderViewModel

<div class="form-group row">
	<label asp-for="@Model.MainModulePlaceHolderId" class="col-md-3 col-form-label"></label>
	<div class="col-md-6">
		<select asp-for="@Model.MainModulePlaceHolderId" class="form-control" asp-items="@(await dropdownService.GetMainModulePlaceHolderList())">
			<option value="">- Select MainModulePlaceHolder -</option>
		</select>
		<span asp-validation-for="@Model.MainModulePlaceHolderId" class="text-danger"></span>
	</div>
</div>
<div class="form-group row">
	<label asp-for="@Model.Code" class="col-md-3 col-form-label"></label>
	<div class="col-md-6">
		<input asp-for="@Model.Code" class="form-control" />
		<span asp-validation-for="@Model.Code" class="text-danger"></span>
	</div>
</div>

