@using Models
@model ApproverSetupViewModel

<div class="col-md-12">
    <label asp-for="@Model.TableName" class="fieldrequired"></label>
    <input asp-for="@Model.TableName" class="form-control" maxlength="450" readonly/>
    <span asp-validation-for="@Model.TableName" class="text-danger"></span>
</div>
<div class="col-md-12">
    <label asp-for="@Model.ApprovalType" class="fieldrequired"></label>
    <select asp-for="@Model.ApprovalType" class="form-control" asp-items="@CompanyNamePlaceHolder.ProjectNamePlaceHolder.Core.ProjectNamePlaceHolder.ApprovalTypes.ApprovalTypeList.Select(l => new SelectListItem { Value = l, Text = l })">
        <option value="">- Select Approval Type -</option>
    </select>
    <span asp-validation-for="@Model.ApprovalType" class="text-danger"></span>
</div>

