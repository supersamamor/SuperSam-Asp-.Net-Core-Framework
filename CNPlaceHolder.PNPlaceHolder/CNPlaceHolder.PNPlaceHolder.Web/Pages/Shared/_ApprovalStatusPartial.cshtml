@model string


@if (Model == CNPlaceHolder.PNPlaceHolder.Core.PNPlaceHolder.ApprovalStatus.ForApproval)
{
    <h4><span class="badge badge-info">@Model</span></h4>
}
else if (Model == CNPlaceHolder.PNPlaceHolder.Core.PNPlaceHolder.ApprovalStatus.PartiallyApproved)
{
    <h4><span class="badge badge-primary">@Model</span></h4>
}
else if (Model == CNPlaceHolder.PNPlaceHolder.Core.PNPlaceHolder.ApprovalStatus.Approved)
{
    <h4><span class="badge badge-success">@Model</span></h4>
}
else if (Model == CNPlaceHolder.PNPlaceHolder.Core.PNPlaceHolder.ApprovalStatus.Rejected)
{
    <h4><span class="badge badge-danger">@Model</span></h4>
}