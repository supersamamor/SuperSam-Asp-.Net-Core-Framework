@model string


@if (Model == CTI.LocationApi.Core.LocationApi.ApprovalStatus.ForApproval)
{
    <h4><span class="badge badge-info">@Model</span></h4>
}
else if (Model == CTI.LocationApi.Core.LocationApi.ApprovalStatus.PartiallyApproved)
{
    <h4><span class="badge badge-primary">@Model</span></h4>
}
else if (Model == CTI.LocationApi.Core.LocationApi.ApprovalStatus.Approved)
{
    <h4><span class="badge badge-success">@Model</span></h4>
}
else if (Model == CTI.LocationApi.Core.LocationApi.ApprovalStatus.Rejected)
{
    <h4><span class="badge badge-danger">@Model</span></h4>
}