@model string


@if (Model == CompanyNamePlaceHolder.ProjectNamePlaceHolder.Core.AreaPlaceHolder.ApprovalStatus.ForApproval)
{
    <h4><span class="badge badge-info">@Model</span></h4>
}
else if (Model == CompanyNamePlaceHolder.ProjectNamePlaceHolder.Core.AreaPlaceHolder.ApprovalStatus.PartiallyApproved)
{
    <h4><span class="badge badge-primary">@Model</span></h4>
}
else if (Model == CompanyNamePlaceHolder.ProjectNamePlaceHolder.Core.AreaPlaceHolder.ApprovalStatus.Approved)
{
    <h4><span class="badge badge-success">@Model</span></h4>
}
else if (Model == CompanyNamePlaceHolder.ProjectNamePlaceHolder.Core.AreaPlaceHolder.ApprovalStatus.Rejected)
{
    <h4><span class="badge badge-danger">@Model</span></h4>
}