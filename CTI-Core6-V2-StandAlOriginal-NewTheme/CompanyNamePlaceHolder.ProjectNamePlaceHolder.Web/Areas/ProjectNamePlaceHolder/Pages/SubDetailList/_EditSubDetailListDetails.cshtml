@using Models
@model SubDetailListViewModel

<div class="col-md-12">
	<label asp-for="@Model.Code" class="fieldrequired"></label>
	<input asp-for="@Model.Code" class="form-control" maxlength="255" />
	<span asp-validation-for="@Model.Code" class="text-danger"></span>
	
</div>
<div class="col-md-12">
	<label asp-for="@Model.TestForeignKeyOne" class="fieldrequired"></label>
	<select asp-for="@Model.TestForeignKeyOne" class="form-control" asp-items="@(dropdownService.GetMainModuleList(Model.TestForeignKeyOne))"><option value="">- Select MainModulePlaceHolder -</option></select>
	<span asp-validation-for="@Model.TestForeignKeyOne" class="text-danger"></span>
	
</div>

