@model string


@if (Model == CompanyNamePlaceHolder.ProjectNamePlaceHolder.Core.ProjectNamePlaceHolder.ApprovalStatus.ForApproval)
{
    <h4><span class="badge bg-info">@Model</span></h4>
}
else if (Model == CompanyNamePlaceHolder.ProjectNamePlaceHolder.Core.ProjectNamePlaceHolder.ApprovalStatus.PartiallyApproved)
{
    <h4><span class="badge bg-primary">@Model</span></h4>
}
else if (Model == CompanyNamePlaceHolder.ProjectNamePlaceHolder.Core.ProjectNamePlaceHolder.ApprovalStatus.Approved)
{
    <h4><span class="badge bg-success">@Model</span></h4>
}
else if (Model == CompanyNamePlaceHolder.ProjectNamePlaceHolder.Core.ProjectNamePlaceHolder.ApprovalStatus.Rejected)
{
    <h4><span class="badge bg-danger">@Model</span></h4>
}