@using Models
@model IList<UserRoleViewModel>
<table id="roles" class="table table-hover table-bordered table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.First().Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.First().Selected)
            </th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count(); i++)
        {
            <tr>
                <td>
                    @Model[i].Name
                </td>
                <td>
                    <div class="form-check m-1">
                        <input asp-for="@Model[i].Selected" class="form-check-input" disabled />
                    </div>
                </td>
            </tr>
        }
    </tbody>
</table>